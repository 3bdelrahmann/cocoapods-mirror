    "ENABLE_BITCODE": "NO",
    "OTHER_LDFLAGS": [
      "-lc++",
      "-llibstdc++.dylib",
      "-lstdc++",
      "-llibz.tbd",
      "-llibresolv.9.tbd"
    ]
  },
  "user_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "VALID_ARCHS": "x86_64 arm64",
    "VALID_ARCHS[sdk=iphoneos*]": "arm64",
    "VALID_ARCHS[sdk=iphonesimulator*]": "x86_64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "ENABLE_BITCODE": "NO"
