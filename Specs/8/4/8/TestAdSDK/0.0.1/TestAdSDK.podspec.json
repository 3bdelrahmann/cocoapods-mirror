{
  "name": "TestAdSDK",
  "version": "0.0.1",
  "summary": "广告聚合sdk",
  "description": "一个广告聚合sdk,聚合国内多家主流广告平台",
  "homepage": "https://gitee.com/fyl007/test-ad-sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "tradplus": "service@tradplus.com"
  },
  "source": {
    "git": "https://gitee.com/fyl007/test-ad-sdk.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "frameworks": [
    "SystemConfiguration",
    "CoreGraphics",
    "Foundation",
    "UIKit"
  ],
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": [
      "-lObjC"
    ]
  },
  "libraries": [
    "c++",
    "z",
    "sqlite3",
    "xml2",
    "resolv"
  ],
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "default_subspecs": "TestAdSDK",
  "subspecs": [
    {
      "name": "TestAdSDK",
      "platforms": {
        "ios": "9.0"
      },
      "vendored_frameworks": "TestAdSDK/TradPlusAds.framework",
      "resources": [
        "TestAdSDK/TradPlusAds.bundle",
        "TestAdSDK/TradPlusADX.bundle"
      ]
    }
  ]
}
