{
  "name": "ACPCampaignClassic",
  "version": "2.1.1",
  "summary": "Campaign Classic framework for Adobe Experience Cloud SDK. Written and maintained by Adobe.",
  "description": "The campaign classic framework provides APIs that allow use of the Campaign Classic product in the V5 Adobe Experience Cloud SDK.",
  "homepage": "https://git.corp.adobe.com/dms-mobile/v5-podspecs",
  "license": {
    "type": "Commercial",
    "text": "Adobe.  All Rights Reserved."
  },
  "authors": "Adobe Mobile SDK Team",
  "source": {
    "git": "git@git.corp.adobe.com:dms-mobile/v5-ios-builds.git",
    "tag": "v2.1.1-ACPCampaignClassic"
  },
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "default_subspecs": "xcframeworks",
  "static_framework": true,
  "cocoapods_version": ">= 1.10",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PROJECT_DIR}/**"
  },
  "dependencies": {
    "ACPCore": [
      ">= 2.9.0"
    ]
  },
  "subspecs": [
    {
      "name": "xcframeworks",
      "ios": {
        "vendored_frameworks": "iOS/ACPCampaignClassic.xcframework",
        "frameworks": [
          "UIKit",
          "SystemConfiguration"
        ],
        "libraries": [
          "sqlite3.0",
          "z",
          "c++"
        ]
      }
    }
  ]
}
