{
  "name": "WCDB.cpp",
  "version": "2.0.0",
  "summary": "WCDB is a cross-platform database framework developed by WeChat.",
  "description": "The WeChat Database, for C++. (If you want to use WCDB for Swift or Objective-C, see the \"WCDB.swift\" pod or \"WCDB.objc\" pod. If you want to use WCDB for C++/Objc/Swift in one project, see the \"WCDB\" pod)\n\nWCDB is an efficient, complete, easy-to-use mobile database framework used in the WeChat application.\nIt can be a replacement for Core Data, SQLite & FMDB.",
  "homepage": "https://github.com/Tencent/wcdb",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": {
    "Qiuwen-Chen": "qwchen2008@163.com"
  },
  "module_name": "WCDB",
  "platforms": {
    "ios": "9.0",
    "osx": "10.9",
    "watchos": "3.0",
    "tvos": "9.0"
  },
  "source": {
    "git": "https://github.com/Tencent/wcdb.git",
    "commit": "9c1476b5b993b64329a30c799124fb0b885efb4b"
  },
  "public_header_files": [
    "apple/cpp/WCDBCpp.h",
    "apple/cpp/**/*.{h,hpp}",
    "apple/common/winq/*.{h,hpp}",
    "apple/common/winq/**/*.{h,hpp}",
    "apple/common/utility/Shadow.hpp",
    "apple/common/utility/Macro.h",
    "apple/common/utility/CaseInsensiveList.hpp",
    "apple/common/core/fts/tokenizer/TokenizerModule.hpp",
    "apple/common/core/fts/tokenizer/TokenizerModuleTemplate.hpp",
    "apple/common/core/fts/tokenizer/BaseTokenizerUtil.hpp",
    "apple/common/core/fts/tokenizer/PinyinTokenizer.hpp",
    "apple/common/core/fts/tokenizer/OneOrBinaryTokenizer.hpp",
    "apple/common/core/fts/auxfunction/FTS5AuxiliaryFunctionTemplate.hpp",
    "apple/common/core/fts/auxfunction/AuxiliaryFunctionModule.hpp",
    "apple/common/core/fts/auxfunction/SubstringMatchInfo.hpp",
    "apple/common/core/fts/FTSError.hpp",
    "apple/common/core/RecyclableHandle.hpp",
    "apple/common/core/Tag.hpp",
    "apple/common/base/Recyclable.hpp",
    "apple/common/base/SharedThreadedErrorProne.hpp",
    "apple/common/base/StringView.hpp",
    "apple/common/base/Optional.hpp",
    "apple/common/base/Error.hpp",
    "apple/common/base/Data.hpp",
    "apple/common/base/UnsafeData.hpp"
  ],
  "source_files": [
    "apple/common/**/*.{h,hpp,c,cpp}",
    "apple/cpp/**/*.{h,hpp,c,cpp}"
  ],
  "exclude_files": "apple/cpp/tests/**",
  "frameworks": [
    "CoreFoundation",
    "Security",
    "Foundation"
  ],
  "ios": {
    "frameworks": "UIKit"
  },
  "libraries": [
    "z",
    "c++"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "SQLITE_WCDB=1 SQLITE_WCDB_LOCK_HOOK=1 SQLITE_WCDB_CHECKPOINT_HANDLER=1 SQLITE_WCDB_SUSPEND=1 SQLITE_HAS_CODEC SQLITE_DEFAULT_PAGE_SIZE=4096 SQLITE_MAX_VARIABLE_NUMBER=99 SQLITE_DEFAULT_SYNCHRONOUS=1 SQLITE_DEFAULT_WAL_SYNCHRONOUS=1 SQLITE_DEFAULT_LOCKING_MODE=0 SQLITE_DEFAULT_WAL_AUTOCHECKPOINT=0",
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/WCDB",
    "LIBRARY_SEARCH_PATHS[sdk=macosx*]": "$(SDKROOT)/usr/lib/system",
    "CLANG_CXX_LANGUAGE_STANDARD": "gnu++14",
    "CLANG_CXX_LIBRARY": "libc++",
    "GCC_C_LANGUAGE_STANDARD": "gnu11",
    "VALID_ARCHS": "arm64e arm64 x86_64",
    "VALID_ARCHS[sdk=watchos*]": "arm64_32 armv7k",
    "VALID_ARCHS[sdk=watchsimulator*]": "i386",
    "OTHER_CFLAGS": "-fvisibility-inlines-hidden",
    "OTHER_CPLUSPLUSFLAGS": "-fvisibility-inlines-hidden"
  },
  "header_dir": "WCDB",
  "dependencies": {
    "WCDBOptimizedSQLCipher": [
      "~> 1.4.0"
    ]
  }
}
