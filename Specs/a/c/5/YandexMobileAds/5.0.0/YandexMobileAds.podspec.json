{
  "name": "YandexMobileAds",
  "version": "5.0.0",
  "summary": "The Yandex Mobile Ads SDK provides mobile interfaces for Yandex Advertising Network",
  "homepage": "https://tech.yandex.ru/mobile-ads/",
  "license": {
    "type": "PROPRIETARY",
    "file": "LICENSE.txt"
  },
  "authors": {
    "Yandex LLC": "mobads@yandex-team.ru"
  },
  "source": {
    "http": "https://storage.mds.yandex.net/get-ads-mobile-sdk/223308/YandexMobileAds-5.0.0-ios-7957b987-50e7-4910-9cd0-b9e957dacb50.zip",
    "sha1": "79b3cbfd819cbff3ce06221e1713b6ea1aa81ae9"
  },
  "libraries": "xml2",
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreTelephony",
    "CoreLocation",
    "CoreGraphics",
    "AdSupport",
    "StoreKit",
    "SystemConfiguration",
    "QuartzCore",
    "AVFoundation",
    "CoreImage",
    "CoreMedia"
  ],
  "weak_frameworks": "WebKit",
  "platforms": {
    "ios": "10.0"
  },
  "default_subspecs": "Static",
  "preserve_paths": "static/YandexMobileAds.xcframework",
  "source_files": "static/YandexMobileAds.xcframework/**/*.h",
  "public_header_files": "static/YandexMobileAds.xcframework/**/*.h",
  "vendored_frameworks": "static/YandexMobileAds.xcframework",
  "resources": "static/YandexMobileAds.xcframework/YandexMobileAdsBundle.bundle",
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -all_load"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS[sdk=iphoneos*]": "arm64 armv7",
    "VALID_ARCHS[sdk=iphonesimulator*]": "i386 x86_64 arm64"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -all_load",
    "VALID_ARCHS[sdk=iphoneos*]": "arm64 armv7",
    "VALID_ARCHS[sdk=iphonesimulator*]": "i386 x86_64 arm64"
  },
  "subspecs": [
    {
      "name": "Static",
      "dependencies": {
        "YandexMobileMetrica": [
          ">= 4.0.0",
          "< 5.0.0"
        ]
      }
    },
    {
      "name": "Dynamic",
      "dependencies": {
        "YandexMobileMetrica/Dynamic": [
          ">= 4.0.0",
          "< 5.0.0"
        ]
      }
    }
  ]
}
