{
  "name": "CDetect",
  "version": "2.0.0",
  "platforms": {
    "ios": "14.0"
  },
  "summary": "This is the CDetect.podspec which can be used to create a CDetect Pod",
  "description": "C Detect iOS Software Development Kit, for use with a Waire Health C Detect Device",
  "homepage": "https://github.com/WaireHealth/iOSDevelopmentKit",
  "documentation_url": "https://github.com/WaireHealth/iOSDevelopmentKit",
  "swift_versions": "5.3",
  "license": {
    "type": "MIT",
    "file": "LICENCE.md"
  },
  "authors": {
    "Waire Health": "ruaridh@waire.health"
  },
  "source": {
    "git": "https://github.com/WaireHealth/iOSDevelopmentKit.git",
    "tag": "2.0.0",
    "submodules": true
  },
  "source_files": "CDetect/**/*.{swift,h,zip,bin,xcassets}",
  "public_header_files": "CDetect/CDetect.h",
  "resources": "CDetect/**/*.{xib,xcassets,json,png,zip,bin}",
  "ios": {
    "resource_bundles": {
      "CDetect": [
        "Assets.xcassets",
        "CDetect/**/*.xcassets"
      ]
    },
    "vendored_frameworks": "CDetect.xcframework"
  },
  "dependencies": {
    "ZIPFoundation": [
      "~> 0.9.11"
    ],
    "iOSDFULibrary": [
      "~> 4.13.0"
    ]
  },
  "swift_version": "5.3"
}
