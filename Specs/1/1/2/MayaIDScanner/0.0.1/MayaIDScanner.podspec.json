{
  "name": "MayaIDScanner",
  "version": "0.0.1",
  "summary": "IDScanner to provide RealTime Image feedback while capturing.",
  "description": "Maya IDScanner is used to scan the CardType IDs and provide real time feedback of blurryness or low light environment while capturing the ID",
  "homepage": "https://gitlab.corp.paymaya.com/core/iam/profile/kyc/kyc-mobile-libraries/ios/mayaidscannerpods",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "p.atul.vishnoi": "p.atul.vishnoi@paymaya.com"
  },
  "source": {
    "git": "https://gitlab.corp.paymaya.com/core/iam/profile/kyc/kyc-mobile-libraries/ios/mayaidscannerpods.git",
    "tag": "0.0.1",
    "submodules": true
  },
  "platforms": {
    "ios": "11.0"
  },
  "swift_versions": "5",
  "source_files": "MayaIDScanner/Classes/**/*",
  "static_framework": true,
  "resource_bundles": {
    "Resources": "MayaIDScanner/Assets/*.{xcassets,xib}",
    "Localizables": "MayaIDScanner/Strings/*.lproj"
  },
  "frameworks": [
    "UIKit",
    "Vision",
    "Accelerate",
    "CoreMedia",
    "AVFoundation"
  ],
  "default_subspecs": "OpenCVWrapper",
  "subspecs": [
    {
      "name": "OpenCVWrapper",
      "source_files": "OpenCVWrapper/**/*.{h,mm}",
      "dependencies": {
        "OpenCV": [
          "~> 4.3.0"
        ]
      }
    }
  ],
  "swift_version": "5"
}
