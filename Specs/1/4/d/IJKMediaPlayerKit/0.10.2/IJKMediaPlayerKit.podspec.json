{
  "name": "IJKMediaPlayerKit",
  "version": "0.10.2",
  "summary": "(新一代 ijkplayer) IJKMediaPlayer framework for iOS and macOS.",
  "description": "macOS/iOS video player based on ijkplayer,using FFmpeg 5.1,support Metal、hw accel、HDR、Subtitle...",
  "homepage": "https://github.com/debugly/ijkplayer",
  "license": {
    "type": "LGPLv2.1",
    "text": "ijkplayer is licensed under LGPLv2.1 or later, so itself is free for commercial use under LGPLv2.1 or later."
  },
  "authors": {
    "MattReach": "qianlongxu@gmail.com"
  },
  "source": {
    "http": "https://github.com/debugly/ijkplayer/releases/download/k0.10.2/IJKMediaPlayerKit-0.10.2.zip"
  },
  "platforms": {
    "osx": "10.13",
    "ios": "11.0"
  },
  "ios": {
    "pod_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
    },
    "user_target_xcconfig": {
      "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
    },
    "frameworks": [
      "UIKit",
      "OpenGLES"
    ],
    "vendored_frameworks": "iOS/IJKMediaPlayerKit.framework"
  },
  "osx": {
    "frameworks": [
      "AppKit",
      "AudioUnit",
      "OpenGL",
      "GLKit",
      "CoreImage"
    ],
    "vendored_frameworks": "macOS/IJKMediaPlayerKit.framework"
  },
  "libraries": [
    "z",
    "iconv",
    "xml2",
    "bz2",
    "c++",
    "lzma"
  ],
  "frameworks": [
    "AVFoundation",
    "AudioToolbox",
    "CoreMedia",
    "CoreVideo",
    "VideoToolbox",
    "Metal"
  ]
}
