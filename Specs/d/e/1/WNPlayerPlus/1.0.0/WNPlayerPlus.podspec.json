{
  "name": "WNPlayerPlus",
  "version": "1.0.0",
  "summary": "A short description of WNPlayerPlus.",
  "description": "'基于WNPlayer开发，原始库地址https://github.com/zhengwenming/WNPlayer.git，项目内使用自编译FFMpeg.Framework，版本4.3.3。'",
  "homepage": "https://github.com/ilongge/WNPlayerPlus",
  "license": {
    "type": "MIT",
    "file": "LICENSE.txt"
  },
  "authors": {
    "ilongge": "1015820857@qq.com"
  },
  "source": {
    "git": "https://github.com/ilongge/WNPlayerPlus.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "ARCHS[sdk=iphonesimulator*]": "$(ARCHS_STANDARD_64_BIT)",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "vendored_frameworks": "Frameworks/FFmpeg.framework",
  "libraries": [
    "bz2",
    "iconv",
    "z"
  ],
  "frameworks": [
    "AVFoundation",
    "AudioToolbox",
    "VideoToolbox",
    "Accelerate",
    "OpenGLES",
    "QuartzCore"
  ],
  "source_files": "WNPlayerPlus/**/*.{h,m}",
  "public_header_files": [
    "WNPlayerPlus/WNPlayerPlus.h",
    "WNPlayerPlus/WNPlayer/*.h"
  ],
  "resource_bundles": {
    "WNPlayerPlus": [
      "WNPlayerPlus/**/*.{xib,xcassets,json,glsl,strings}"
    ]
  }
}
