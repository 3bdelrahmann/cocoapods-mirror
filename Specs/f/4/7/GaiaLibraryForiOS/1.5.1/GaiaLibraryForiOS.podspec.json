{
  "name": "GaiaLibraryForiOS",
  "version": "1.5.1",
  "summary": "盖雅内部iOS静态库",
  "description": "TODO: Add long description of the pod here.",
  "homepage": "http://git.gaiaworks.cn:8088/gaia-mobile/GaiaLibraryForiOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yuki.lan": "yuki.lan@gaiaworks.cn"
  },
  "source": {
    "git": "http://git.gaiaworks.cn:8088/gaia-mobile/GaiaLibraryForiOS.git",
    "tag": "1.5.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "GaiaLibraryForiOS/Classes/**/*",
  "public_header_files": "Pod/Classes/**/*.h",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "resource_bundles": {
    "GaiaLibraryForiOS": [
      "GaiaLibraryForiOS/Assets/*.png",
      "GaiaLibraryForiOS/Resource/**/*"
    ]
  },
  "resources": [
    "GaiaLibraryForiOS/bundles/GaiaTZImagePickerController.bundle"
  ],
  "vendored_libraries": "GaiaLibraryForiOS/Classes/Libraries/BrightSDK/BrightSDK/libBrightSDK.a",
  "libraries": "bz2.1.0",
  "compiler_flags": "-DNDEBUG -DSQLITE_HAS_CODEC -DSQLITE_TEMP_STORE=2 -DSQLITE_SOUNDEX  -DSQLITE_THREADSAFE -DSQLITE_ENABLE_RTREE -DSQLITE_ENABLE_STAT3 -DSQLITE_ENABLE_STAT4 -DSQLITE_ENABLE_COLUMN_METADATA -DSQLITE_ENABLE_MEMORY_MANAGEMENT -DSQLITE_ENABLE_LOAD_EXTENSION -DSQLITE_ENABLE_FTS4 -DSQLITE_ENABLE_FTS4_UNICODE61 -DSQLITE_ENABLE_FTS3_PARENTHESIS -DSQLITE_ENABLE_UNLOCK_NOTIFY -DSQLITE_ENABLE_JSON1 -DSQLITE_ENABLE_FTS5 -DSQLCIPHER_CRYPTO_CC -DHAVE_USLEEP=1 -DSQLITE_MAX_VARIABLE_NUMBER=99999",
  "xcconfig": {
    "OTHER_CFLAGS": "$(inherited) -DSQLITE_HAS_CODEC -DSQLITE_THREADSAFE -DSQLITE_TEMP_STORE=2 -DSQLCIPHER_CRYPTO_CC -DSQLITE_SOUNDEX -DSQLITE_ENABLE_RTREE -DSQLITE_ENABLE_STAT3 -DSQLITE_ENABLE_STAT4 -DSQLITE_ENABLE_COLUMN_METADATA -DSQLITE_ENABLE_MEMORY_MANAGEMENT -DSQLITE_ENABLE_LOAD_EXTENSION -DSQLITE_ENABLE_FTS4 -DSQLITE_ENABLE_FTS4_UNICODE61 -DSQLITE_ENABLE_FTS3_PARENTHESIS -DSQLITE_ENABLE_UNLOCK_NOTIFY -DSQLITE_ENABLE_JSON1 -DSQLITE_ENABLE_FTS5 -DHAVE_USLEEP=1 -DSQLITE_MAX_VARIABLE_NUMBER=99999",
    "OTHER_LDFLAGS": "$(inherited) -framework Security",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) SQLITE_HAS_CODEC=1 GAIALIBRARY_USE_GAODE=1"
  },
  "dependencies": {
    "AMap2DMap-NO-IDFA": [
      "5.6.1"
    ],
    "AMapSearch-NO-IDFA": [
      "7.6.0"
    ],
    "AMapLocation-NO-IDFA": [
      "2.8.0"
    ],
    "KSCrash": [
      "1.15.25"
    ]
  }
}
